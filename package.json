{
  "name": "@nezuchan/redis-collection",
  "version": "1.3.0",
  "description": "A Fork of @discordjs/redis-collection.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "build": "swc src -d dist && npm run build:typings && npm run build:esm",
    "build:typings": "tsc --emitDeclarationOnly --declaration",
    "build:esm": "gen-esm-wrapper dist/index.js dist/index.mjs"
  },
  "author": "KagChi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@hazmi35/eslint-config": "^8.7.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.20",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "gen-esm-wrapper": "^1.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {}
  },
  "dependencies": {
    "@discordjs/collection": "^1.3.0",
    "ioredis": "^5.2.4"
  }
}
